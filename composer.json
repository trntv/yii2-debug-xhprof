{
    "name": "trntv/yii2-debug-xhprof",
    "description": "xhprof panel for Yii2 debug module",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","yii2-debug","panel","xhprof"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Eugine Terentev",
            "email": "eugine@terentev.net"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-debug": "*"
    },
    "suggest": {
        "ext-xhprof": "Xhprof PHP profiler"
    },
    "autoload": {
        "psr-4": {
            "trntv\\debug\\xhprof\\": ""
        }
    },
    "extra": {
        "bootstrap": "trntv\\debug\\xhprof\\panels\\XhprofPanel"
    }
}
